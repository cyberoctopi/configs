escape C-f
startup_message off

set winname class
defborder 0
defpadding 0 0 0 0
defbarpadding 0 0 
set font -nil-profont-medium-r-normal--0-0-72-72-c-0-iso8859-1
set border 0
set bgcolor grey10
set fgcolor #E20014
gravity se
set waitcursor 0


exec xsetroot -cursor_name center_ptr
exec clipbored
#exec rmshit
#exec rpd
exec unclutter -idle 1 -root
exec urxvt -name URxvt.india
hsplit
focusright
exec urxvt -name URxvt.india
vsplit
focusright
exec xterm
vsplit
focusdown
exec urxvt -name URxvt.india
focusdown
exec urxvt -name URxvt.india
vsplit
exec urxvt -name URxvt.india

bind C-f-k focusup 
bind space exec ratpoison -c "echo $(rpc npi)"
bind dead_acute exec rpws init 2 -ak
bind = exec ratpoison -c "setenv fs1 $(ratpoison -c 'fdump')"
bind 0 exec ratpoison -c "frestore $(ratpoison -c 'getenv fs1')"
bind h focusleft
bind j focusdown
bind k focusup
bind l focusright
#bind H rpwsp
#bind L rpwsn
bind H exchangeleft
bind J exchangedown
bind K exchangeup
bind L exchangeright
bind c exec urxvtc -name URxvt.india
bind e exec sh dmenushit
bind t exec perl $HOME/bin/expose
bind p exec ssh scp1@192.168.1.101 -p 19216 'mpc next'
bind o exec rmcd -t
bind BackSpace rmcd -l shiva
#bind p exec rmcd -n
#bind o exec mplayerd -cmd toggle


