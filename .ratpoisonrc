escape C-f
startup_message off

set winname class
defborder 0
defpadding 0 0 0 0
defbarpadding 0 0 
set font -nil-profont-medium-r-normal--0-0-72-72-c-0-iso8859-1
set border 0
set bgcolor black
set fgcolor red
gravity se

#exec xmodmap -quiet ~/configs/.Xmodmap
exec xsetroot -cursor_name left_ptr
exec unclutter -idle 0 -root
exec urxvt -name URxvt.india
hsplit
hsplit
focusright
exec urxvt name URxvt.india
focusright
vsplit

bind = exec ratpoison -c "setenv fs1 $(ratpoison -c 'fdump')"
bind 0 exec ratpoison -c "frestore $(ratpoison -c 'getenv fs1')"
bind h focusleft
bind j focusdown
bind k focusup
bind l focusright
bind H rpwsp
bind L rpwsn
bind c exec urxvtc -name URxvt.india
bind e exec sh dmenushit

rpws init 6 -ak
