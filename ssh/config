# vim: set ft=sshconfig cc=0 fdm=marker:
host *
  controlmaster           auto
  controlpath             ~/.ssh/connections/%r_%h_%p
  controlpersist          yes
  GSSAPIAuthentication    no
host sid                  #{{{
  user                    scp1
  hostname                192.168.1.3
  port                    16813
  compression             no
  ciphers                 arcfour
  checkhostip             no
  connecttimeout          3
  permitlocalcommand      yes
  localcommand            date;pwc
  numberofpasswordprompts 10
#}}}
